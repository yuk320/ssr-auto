<service>
	<id>ssrautoreflash.exe</id>
	<name>SSR Auto Reflash</name>
	<description>SSR Service List Auto Reflash At 0,6,12,18</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>E:\Tools\SSR\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>E:\Tools\SSR\index.js</argument>
	<argument>--log</argument>
	<argument>SSR Auto Reflash wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>E:\Tools\SSR</workingdirectory>
</service>